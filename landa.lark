start: line*

line: call _NEWLINE?

call: FUNCTION_NAME "(" parameters ")"

parameters: parameter ("," " "* parameter)* 

parameter: PARAMETER_NAME ":" " "* PARAMETER_VALUE

FUNCTION_NAME: CNAME
PARAMETER_NAME: CNAME
PARAMETER_VALUE: ESCAPED_STRING | INT
_NEWLINE: "\n"

%import common.CNAME
%import common.ESCAPED_STRING
%import common.INT
